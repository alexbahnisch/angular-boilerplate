{
  "name": "angular-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate for creating angular/webpack apps.",
  "license": "MIT",
  "author": "Alex Bahnisch",
  "homepage": "https://github.com/alexbahnisch/angular-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexbahnisch/angular-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/alexbahnisch/angular-boilerplate/issues"
  },
  "main": "./src/package/index.ts",
  "private": true,
  "keywords": [
    "boilerplate",
    "npm",
    "angular",
    "typescript",
    "webpack"
  ],
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@types/node": "^10.11.7",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.5.7",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "live-server": "^1.2.0",
    "rxjs": "^6.3.3",
    "typescript": "^3.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-livereload-plugin": "^2.1.1",
    "zone.js": "^0.8.26"
  },
  "peerDependencies": {},
  "scripts": {
    "build": "npm run clean && cross-env webpack --progress -p",
    "clean": "rm -rf ./dist",
    "server": "live-server ./dist/ --entry-file=index.html --port=8080",
    "watch": "npm run clean && webpack --devtool inline-source-map --progress --watch"
